{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "fileLoggingMode": "always",
        "logLevel": {
            "EPR.EventDispatcher": "Information",
            "default": "Warning"
        }
    },
    "extensions": {
        "serviceBus": {
            "messageHandlerOptions": {
                "autoComplete": false
            }
        }
    },
    "functionTimeout": "00:01:00"
}